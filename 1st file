void main() {
    const min = 60;
    var userName = 'apoorv_21';
    var followerCount =  400;
    var followingCount = 500;
    var watchTime = 40.9;
    var hours = watchTime.toInt();
    var minutes = (watchTime - hours) *min;
    
    var posts = 10;
    var bio = 'I am a mobile developer \n Currently learning it :)';
    print('Username : $userName');
